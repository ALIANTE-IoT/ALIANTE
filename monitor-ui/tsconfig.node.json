{
    "compilerOptions": {
        "composite": true,
        "target": "ESNext",
        "lib": ["ESNext"],
        "module": "ESNext",
        "moduleResolution": "Node",
        "types": ["node"],
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "strict": true,
        "skipLibCheck": true
    },
    "include": ["vite.config.ts"]
}
